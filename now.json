{
  "version": 2,
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "server.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/static/robots.txt" },
    { "src": "/preview", "dest": "/preview" },
    { "src": "/(?<uid>[^/]+)$", "dest": "/page?uid=$uid" }
  ],
  "env": {
    "STATICKIT": "@statickit",
    "PRISMIC": "@prismic"
  },
  "build": {
    "env": {
      "STATICKIT": "@statickit",
      "PRISMIC": "@prismic"
    }
  }
}
